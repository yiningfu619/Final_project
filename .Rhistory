# Left 5% cutoff (lower tail)
qbinom(0.05, size = 100, prob = 0.5)
# Right 5% cutoff (upper tail)
qbinom(0.95, size = 100, prob = 0.5)
# Set parameters
n <- 100
p <- 0.5
N <- 200
K <- 100
sample_size <- 100
# Range of x to check (e.g., 40 to 60)
x_vals <- 40:60
# Calculate f0(x) = Binomial(n = 100, p = 0.5)
f0 <- dbinom(x_vals, size = n, prob = p)
# Calculate f1(x) = Hypergeometric(N = 200, K = 100, n = 100)
f1 <- dhyper(x_vals, m = K, n = N - K, k = sample_size)
# Calculate likelihood ratio: LR(x) = f1(x) / f0(x)
LR <- f1 / f0
# Combine into a data frame
result_table <- data.frame(
x = x_vals,
`f0(x)` = round(f0, 6),
`f1(x)` = round(f1, 6),
`LR(x)` = round(LR, 6)
)
# Print the table
print(result_table)
